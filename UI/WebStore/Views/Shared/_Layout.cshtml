<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" charset="utf-8">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"]</title>

    <link href="~/bootstrap/dist/css/bootstrap.css" rel="stylesheet">
    <link href="~/font-awesome/css/font-awesome.css" rel="stylesheet">

    <environment include="Development">
        <link href="~/css/main.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/prettyPhoto.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/price-range.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/responsive.css" rel="stylesheet" asp-append-version="true">
        <link href="~/css/animate.css" rel="stylesheet" asp-append-version="true">
    </environment>
    <environment exclude="Development">
        <link href="~/css/site.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>

    @await RenderSectionAsync("styles", false)
    <!--[if lt IE 9]>
        <script src="~/js/html5shiv.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="~/images/ico/favicon.ico">
    <script src="~/js/jquery.js"></script>
</head><!--/head-->
<body>
    @await Html.PartialAsync("Partial/_Header")

    <section>
        <div class="container">
            @RenderBody()
        </div>
    </section>

    @await Html.PartialAsync("Partial/_Footer")

    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.scrollUp.min.js"></script>
    <environment include="Development">
        <script src="~/js/price-range.js" asp-append-version="true"></script>
        <script src="~/js/jquery.prettyPhoto.js" asp-append-version="true"></script>
        <script src="~/js/main.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @await RenderSectionAsync("scripts", false)
</body>
</html>